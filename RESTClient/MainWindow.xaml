<Window x:Class="RESTClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="181.194" Width="251.33"
        >
    <Grid>
        <Button Command="{Binding DownloadButtonDelegateCommand}"  Content="Download" HorizontalAlignment="Left" Margin="31,82,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="Download zip database archive file" HorizontalAlignment="Left" Margin="31,51,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="31,105,0,0" TextWrapping="Wrap" Text="{Binding Result, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="194" IsReadOnly="True"/>

    </Grid>
</Window>
